@model Setting
<div class="row justify-content-center">
    <div class="col-md-8">
        <h1>Create Setting Data</h1>
        <form asp-action="create" method="post">
            <div class="form-group">
                <label asp-for="Key">Key*</label>
                <input class="form-control" asp-for="Key">
                <span class="text-danger" asp-validation-for="Key"></span>
            </div>
            <div class="form-group">
                <label asp-for="Value">Value*</label>
                <input class="form-control" asp-for="Value">
                <span class="text-danger" asp-validation-for="Value"></span>
            </div>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>

